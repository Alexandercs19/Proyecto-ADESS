@model Excluidos

@{
    ViewData["Title"] = "Formulario para Excluidos";
}

<div class="card" style="text-align:center; font-size: 25px;">
    <div class="card-header">
        Formulario para Excluidos
    </div>
</div>
<form asp-action="Guardar" asp-controller="MantenedorExcluidos" method="post">
    <div class="form-group">
        <label class="exampleInputEmail1">Introduzca la Cedula</label>
        <input asp-for="Cedula" name ="cedula" type="text" class="form-control">
    </div>
    <div class="form-group">
        <label class="exampleInputEmail1">Introduzca el Motivo</label>
        <input asp-for="Motivo" type="text" class="form-control">
    </div>
    <div class="form-group">
        <label class="exampleInputEmail1">Introduzca el Fecha</label>
        <input asp-for="Fecha" type="text" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary" style="position: relative; top:5px;">Guardar</button>
    <button class="btn btn-warning" asp-action="Listar" style="position: relative; top:5px;">Volver a la lista</button>
</form>

